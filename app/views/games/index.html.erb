<h1>Open Games</h1>
<% @open_games.each do |game| %>
  <a href="<%=join_game_url(game)%>"><%= game.player1.email %>'s game</a>
<% end %>

<form id="new_game_form" action="<%= games_url %>" method="post">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token -%>
  <input type="submit" id="submit-button" value="Create Game">
</form>

<h2>Full Games</h2>
<% @full_games.each do |game| %>
  <%= game.player1.email %> is playing <%= game.player2.email %>
<% end %>
