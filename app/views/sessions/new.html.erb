<form action="<%= session_url %>" method="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <input
    type="text"
    name="email"
    id="user-email"
    value="<%= params[:email] %>"
    placeholder="Email">

  <input
    type="password"
    name="password"
    id="user-password"
    placeholder="Password">


  <input type="submit" value="Sign In">
</form>

<a href="<%=new_user_url%>">Sign Up</a>
